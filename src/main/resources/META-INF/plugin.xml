<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <id>com.github.nadimmussadaud.intellijrustplugin</id>
    <name>IntelliJRustPlugin</name>
    <vendor>nadimmussadaud</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.github.nadimmussadaud.intellijrustplugin.toolWindow.MyToolWindowFactory" id="MyToolWindow"/>
        <postStartupActivity implementation="com.github.nadimmussadaud.intellijrustplugin.startup.MyProjectActivity" />
        <configurationType
                implementation="com.github.nadimmussadaud.intellijrustplugin.CustomExecutableConfigurationType"/>
    </extensions>
</idea-plugin>
